{
  "name": "with",
  "version": "1.0.0",
  "description": "coffee",
  "main": "index.js",
  "scripts": {
    "build": "npx hardhat compile",
    "test": "export TS_NODE_TRANSPILE_ONLY=1 && npx hardhat test",
    "coverage": "npx hardhat coverage --network coverage",
    "docs": "scripts/docs.sh",
    "lint": "npm run lint:js && npm run lint:sol",
    "lint:js": "eslint '*/**/*.{js,ts}' --quiet --fix",
    "lint:sol": "solhint --max-warnings 0 \"contracts/**/*.sol\"",
    "prettier": "prettier --write **/*.sol",
    "vertigo": "scripts/vertigo.sh",
    "analyze": "slither ./contracts",
    "deploy:rinkeby": "npx hardhat run --network rinkeby scripts/deploy.js"
  },
  "author": "yoonsung",
  "license": "ISC",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-solhint": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.11.10",
    "buidler-ethers-v5": "^0.2.3",
    "buidler-gas-reporter": "^0.1.4",
    "chai": "^4.2.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eth-gas-reporter": "^0.2.18",
    "ethereum-waffle": "^3.1.2",
    "ethers": "^5.0.19",
    "hardhat": "^2.0.2",
    "mocha": "^8.2.0",
    "prettier": "^1.19.1",
    "prettier-plugin-solidity": "^1.0.0-alpha.59",
    "solidity-coverage": "^0.7.11",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
